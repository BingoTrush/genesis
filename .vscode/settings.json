{
    "eslint.options": {
        "overrideConfigFile": ".eslintrc.js"
    },
    "stylelint.validate": [
        "css",
        "less",
        "html",
        "vue"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "eslint.alwaysShowStatus": false,
    "stylelint.configFile": "stylelint.config.js",
    "stylelint.packageManager": "yarn",
    "eslint.packageManager": "yarn",
}